{
	"name": "sqltosqlcopy_pipeline",
	"properties": {
		"activities": [
			{
				"name": "sqltosqlcopy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderStoredProcedureName": "[dbo].[getprodbycolor]",
						"storedProcedureParameters": {
							"color": {
								"type": "String",
								"value": {
									"value": "@pipeline().parameters.color",
									"type": "Expression"
								}
							}
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "sqldynamicsource",
						"type": "DatasetReference",
						"parameters": {
							"dd_schemaname": {
								"value": "@pipeline().parameters.pp_schemaname",
								"type": "Expression"
							},
							"dd_tablename": {
								"value": "@pipeline().parameters.pp_tablename_input",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "sqldynamicsource",
						"type": "DatasetReference",
						"parameters": {
							"dd_schemaname": {
								"value": "@pipeline().parameters.pp_schemaname",
								"type": "Expression"
							},
							"dd_tablename": {
								"value": "@pipeline().parameters.pp_tablename_output",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "predelete",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase1",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "NonQuery",
							"text": {
								"value": "@CONCAT('IF OBJECT_ID(',pipeline().parameters.pp_schemaname,'.',pipeline().parameters.pp_tablename_output,',''''U'''') IS NOT NULL\nBEGIN\n    TRUNCATE TABLE ',pipeline().parameters.pp_schemaname,'.',pipeline().parameters.pp_tablename_output,';\nEND')",
								"type": "Expression"
							}
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			}
		],
		"parameters": {
			"pp_schemaname": {
				"type": "string"
			},
			"pp_tablename_input": {
				"type": "string"
			},
			"pp_tablename_output": {
				"type": "string"
			},
			"color": {
				"type": "string"
			}
		},
		"folder": {
			"name": "adfpipeline2"
		},
		"annotations": []
	}
}