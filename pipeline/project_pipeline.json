{
	"name": "project_pipeline",
	"properties": {
		"activities": [
			{
				"name": "landing2input",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings"
							}
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "parameterzipdataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "parametercsvdataset",
						"type": "DatasetReference",
						"parameters": {
							"ds1_container": {
								"value": "@pipeline().parameters.pp_container",
								"type": "Expression"
							},
							"ds1_directory": {
								"value": "@pipeline().parameters.pp_output_folder",
								"type": "Expression"
							},
							"ds1_filename": {
								"value": "@replace(pipeline().parameters.pp_file_name,'.txt','.csv')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pp_container": {
				"type": "string"
			},
			"pp_input_folder": {
				"type": "string"
			},
			"pp_file_name": {
				"type": "string"
			},
			"pp_output_folder": {
				"type": "string"
			}
		},
		"folder": {
			"name": "adfpipeline2"
		},
		"annotations": []
	}
}