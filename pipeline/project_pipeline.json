{
	"name": "project_pipeline",
	"properties": {
		"activities": [
			{
				"name": "landing2input",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings"
							}
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Binary1",
						"type": "DatasetReference",
						"parameters": {
							"ds_container": {
								"value": "@pipeline().parameters.pp_container",
								"type": "Expression"
							},
							"ds_folder": {
								"value": "@pipeline().parameters.pp_input_folder",
								"type": "Expression"
							},
							"ds_filename": {
								"value": "@pipeline().parameters.pp_file_name",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "parametercsvdataset",
						"type": "DatasetReference",
						"parameters": {
							"ds1_container": {
								"value": "@pipeline().parameters.pp_container",
								"type": "Expression"
							},
							"ds1_directory": {
								"value": "@pipeline().parameters.pp_output_folder",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pp_container": {
				"type": "string"
			},
			"pp_input_folder": {
				"type": "string"
			},
			"pp_file_name": {
				"type": "string"
			},
			"pp_output_folder": {
				"type": "string"
			}
		},
		"folder": {
			"name": "adfpipeline2"
		},
		"annotations": []
	}
}